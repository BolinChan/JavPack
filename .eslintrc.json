{
  "extends": ["alloy"],
  "env": {
    "browser": true
  },
  "globals": {
    "request": true,
    "taskQueue": true,
    "fetchStudio": true,
    "fetchJavspyl": true,
    "fetchBlogJav": true,
    "fetchJavStore": true,
    "codeParse": true,
    "files": true,
    "filesSearch": true,
    "GM_addStyle": true,
    "GM_openInTab": true,
    "GM_xmlhttpRequest": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-param-reassign": "off"
  },
  "ignorePatterns": ["**/*.md"]
}
