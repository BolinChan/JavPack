{
  "extends": ["alloy"],
  "env": {
    "browser": true,
    "es6": true,
    "greasemonkey": true
  },
  "globals": {
    "Req": true,
    "Req115": true,
    "Util": true,
    "Util115": true
  },
  "rules": {
    "no-template-curly-in-string": "off",
    "no-param-reassign": "off",
    "no-unused-vars": "warn",
    "complexity": "off",
    "eqeqeq": "off",
    "max-nested-callbacks": "warn"
  },
  "ignorePatterns": ["**/*.md"]
}
