{
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": true
  },
  "cSpell.words": [
    "accessid",
    "aliyuncs",
    "anxia",
    "blogjav",
    "Btns",
    "caribbeancom",
    "errcode",
    "fancybox",
    "fanza",
    "freepv",
    "HEYZO",
    "javdb",
    "javpack",
    "Javspyl",
    "javstore",
    "jdbstatic",
    "Msin",
    "muramura",
    "musume",
    "natsort",
    "ncensored",
    "Neue",
    "pacopacomama",
    "pickcode",
    "pixhost",
    "pondo",
    "proid",
    "savepath",
    "scid",
    "vcode",
    "vrlite",
    "vrsample",
    "xmlhttp"
  ]
}
